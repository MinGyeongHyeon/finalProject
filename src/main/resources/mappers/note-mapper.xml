  	<select id="selectKinderArr" parameterType="noteKindergarden" resultMap="noteKindergarden">
  		SELECT U.USER_NO, U.USER_NAME, K.KINDER_NO, K.KINDER_NAME, K.KINDER_ADDRESS
  		FROM USERS U
  		JOIN KINDERGARDEN K ON(U.USER_NO = K.KINDER_NO)
  	</select>

</mapper>